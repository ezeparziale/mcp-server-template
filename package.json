{
  "name": "mcp-server-template",
  "version": "1.0.0",
  "description": "MCP template",
  "keywords": [
    "modelcontextprotocol",
    "mcp",
    "mcp-server",
    "typescript"
  ],
  "author": "",
  "main": "./build/index.ts",
  "type": "module",
  "scripts": {
    "server:build": "tsc",
    "server:start": "cross-env NODE_ENV=production tsx build/index.js",
    "server:build:watch": "cross-env NODE_ENV=development tsc --watch",
    "server:dev": "cross-env NODE_ENV=development tsx src/index.ts",
    "server:inspect": "npx @modelcontextprotocol/inspector npm run server:dev",
    "inspector": "npx @modelcontextprotocol/inspector",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,mjs,ts,tsx,mts}": "prettier --write"
  },
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@modelcontextprotocol/inspector": "0.16.8",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/node": "24.5.2",
    "cross-env": "10.0.0",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "jiti": "2.5.1",
    "lint-staged": "16.1.6",
    "prettier": "3.6.2",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.18.1",
    "cors": "2.8.5",
    "dotenv": "17.2.2",
    "express": "5.1.0",
    "winston": "3.17.0",
    "zod": "3.25.76"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.7.1"
}
